<h1>Standings</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Wins</th>
      <th>Losses</th>
      <th>Win Pct</th>
    </tr>
  </thead>
  <tbody>
    <% standings.each do |standing| %>
      <tr>
        <td><%= standing.name %></td>
        <td><%= standing.wins %></td>
        <td><%= standing.losses %></td>
        <td><%= standing.win_pct %></td>
      </tr>
    <% end %>
  </tbody>
</table>